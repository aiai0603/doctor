
page{
	width: 100%;
	height:100%;
}
.main{
	height: 100%;
	width: 100%;
}
.pic{
	width: 250rpx;
	height: 250rpx;
}
.logo {
	width: 100%;
	height: 50%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-flow: column;
	        flex-flow: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.title{
	font-weight: 600;
	font-size: 40rpx;
	margin: 10rpx;
	color: #333333;
}
.but {
	width: 80%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-flow: row;
	        flex-flow: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	color: white;
	background-color: #06b908;
	border-radius: 50rpx;
	font-size: 30rpx;
	height: 80rpx;
}
.icon {
	width: 40rpx;
	height: 40rpx;
	margin-right: 10rpx;
}

